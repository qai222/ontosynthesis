    <!-- Reaction Experiment 1 -->
    <ontoreaction:ReactionExperiment rdf:about="#ReactionExperiment1">
        <ontoreaction:isOccurenceOf rdf:resource="#ChemicalReaction1"/>
        <ontoreaction:hasInputChemical rdf:resource="#MethyleneChloride"/>
        <ontoreaction:hasInputChemical rdf:resource="#Ethylenediamine"/>
        <ontoreaction:hasInputChemical rdf:resource="#MethacrylicChloride"/>
        <ontoreaction:hasRxnTemperature rdf:resource="#RoomTemperature"/>
        <ontoreaction:hasOutputChemical rdf:resource="#AminoethylmethacrylamideHydrochloride"/>
        <ontoreaction:hasReactionCondition rdf:resource="#StirringInIceBath"/>
    </ontoreaction:ReactionExperiment>

    <!-- Reaction Experiment 2 -->
    <ontoreaction:ReactionExperiment rdf:about="#ReactionExperiment2">
        <ontoreaction:isOccurenceOf rdf:resource="#ChemicalReaction2"/>
        <ontoreaction:hasInputChemical rdf:resource="#IonExchangedWater"/>
        <ontoreaction:hasInputChemical rdf:resource="#AminoethylmethacrylamideHydrochloride"/>
        <ontoreaction:hasInputChemical rdf:resource="#AnionicSurfaceActiveAgent"/>
