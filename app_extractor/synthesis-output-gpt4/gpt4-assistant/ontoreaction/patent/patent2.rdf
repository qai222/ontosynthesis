        <ontoreaction:hasOutputChemical rdf:resource="#AminoethylmethacrylamideHydrochlorideMethylMethacrylateCopolymer"/>
        <ontoreaction:hasSolvent rdf:resource="#Water"/>
    </ontoreaction:ReactionExperiment>

    <!-- Input Chemicals -->
    <ontoreaction:InputChemical rdf:about="#MethyleneChloride"/>
    <ontoreaction:InputChemical rdf:about="#Ethylenediamine"/>
    <ontoreaction:InputChemical rdf:about="#MethacrylicChloride"/>
    <ontoreaction:InputChemical rdf:about="#IonExchangedWater"/>
    <ontoreaction:InputChemical rdf:about="#AminoethylmethacrylamideHydrochloride"/>
    <ontoreaction:InputChemical rdf:about="#AnionicSurfaceActiveAgent"/>

    <!-- Reaction Conditions -->
    <ontoreaction:ReactionCondition rdf:about="#StirringInIceBath">
        <rdfs:label>Stirred in an ice bath</rdfs:label>
    </ontoreaction:ReactionCondition>
    <ontoreaction:ReactionCondition rdf:about="#HeatedTo50">
        <rdfs:label>Heated reaction up to 50 degrees Celsius</rdfs:label>
    </ontoreaction:ReactionCondition>

    <!-- Solvent -->
    <ontoreaction:Solvent rdf:about="#Water"/>

    <!-- Reaction Temperatures -->
    <ontoreaction:ReactionTemperature rdf:about="#RoomTemperature">
        <rdfs:label>Room Temperature</rdfs:label>
    </ontoreaction:ReactionTemperature>
    <ontoreaction:ReactionTemperature rdf:about="#HeatedTo50">
        <rdfs:label>50Â°C</rdfs:label>
    </ontoreaction:ReactionTemperature>

    <!-- Output Chemical -->
    <ontoreaction:OutputChemical rdf:about="#AminoethylmethacrylamideHydrochloride">
        <rdfs:label>Aminoethylmethacrylamide hydrochloride</rdfs:label>
    </ontoreaction:OutputChemical>
    <ontoreaction:OutputChemical rdf:about="#AminoethylmethacrylamideHydrochlorideMethylMethacrylateCopolymer">
        <rdfs:label>Aminoethylmethacrylamide hydrochloride/methyl methacrylate copolymer</rdfs:label>
    </ontoreaction:OutputChemical>

    <!-- Chemical Reactions -->
    <ontoreaction:ChemicalReaction rdf:about="#ChemicalReaction1"/>
    <ontoreaction:ChemicalReaction rdf:about="#ChemicalReaction2"/>
